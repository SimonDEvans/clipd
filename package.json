{
  "name": "clipd",
  "version": "1.0.0",
  "description": "Puny copy to clipboard library",
  "license": "MIT",
  "main": "index.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/tiaanduplessis/clipd.git"
  },
  "homepage": "https://github.com/tiaanduplessis/clipd",
  "bugs": "https://github.com/tiaanduplessis/clipd/issues",
  "author": {
    "name": "Tiaan",
    "email": "tiaanduplessis@hotmail.com",
    "url": "tiaanduplessis.co.za"
  },
  "scripts": {
    "start": "npm run build; npm run build:min",
    "build": "webpack",
    "build:min": "WEBPACK_ENV=production webpack -p",
    "dev": "webpack --progress --colors --watch",
    "pretest": "npm run lint",
    "test": "jest src/",
    "test:watch": "jest src/ --watch",
    "lint": "standard src/",
    "lint:fix": "npm run lint -- --fix",
    "coverage": "jest src/ --coverage",
    "precommit": "npm test"
  },
  "keywords": [
    "clipboard",
    "copy",
    "clipd"
  ],
  "dependencies": {},
  "devDependencies": {
    "babel-core": "^6.23.1",
    "babel-jest": "^19.0.0",
    "babel-loader": "^6.4.0",
    "babel-plugin-add-module-exports": "^0.2.1",
    "babel-preset-es2015": "^6.22.0",
    "husky": "^0.13.2",
    "jest": "^19.0.2",
    "standard": "^9.0.1",
    "webpack": "^2.2.1"
  },
  "babel": {
    "presets": [
      "es2015"
    ],
    "plugins": [
      "babel-plugin-add-module-exports"
    ]
  },
  "standard": {
    "ignore": [
      "lib/**"
    ]
  }
}
